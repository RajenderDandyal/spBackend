{
  "name": "sapient-backend",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run prod",
    "dev": "cross-env NODE_ENV=development npm run watch ./src/index.js",
    "prod": "npm run build && node ./dist/index.js",
    "build": "npm run clean && babel ./src --out-dir dist",
    "clean": "rm -rf dist && mkdir dist",
    "watch": "nodemon --config ./nodemon.json --",
    "lint": "eslint --debug src/",
    "lint:fix": "eslint --debug src/ --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "prt": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.0",
    "express-sanitizer": "^1.0.5",
    "helmet": "^3.22.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.6.9",
    "morgan": "~1.9.0",
    "rimraf": "^2.6.3",
    "serialize-javascript": "^2.1.1",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-jest-enzyme": "^7.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  }
}
